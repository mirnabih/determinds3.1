{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "**/*.prisma",
      {
        "include": "../generated/**/*",
        "exclude": "**/*.ts",
        "watchAssets": true
      }
    ],
    "watchAssets": true
  },
  "webpack": false,
  "typeCheck": false
}
